{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Orders",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "stripe_id": {
      "type": "string"
    },
    "products": {
      "type": "json"
    },
    "coupon_used": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::coupon.coupon"
    },
    "total": {
      "type": "decimal",
      "default": 0,
      "required": true
    },
    "shipping_status": {
      "type": "string",
      "required": true,
      "default": "pending"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    },
    "address": {
      "type": "json"
    },
    "payment_intent": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::payment-intent.payment-intent"
    },
    "order_id": {
      "type": "string"
    },
    "order_status": {
      "type": "string"
    },
    "refund_requested": {
      "type": "boolean",
      "default": false
    },
    "order_date": {
      "type": "datetime"
    },
    "payment_method": {
      "type": "string",
      "required": false
    }
  }
}
