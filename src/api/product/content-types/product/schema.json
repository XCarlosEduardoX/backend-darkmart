{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Products",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "product_name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "product_name",
      "required": true
    },
    "active": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false,
      "default": false
    },
    "is_featured": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": false
    },
    "price": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false
    },
    "stock": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "discount": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": 0
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "variations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::variation.variation",
      "mappedBy": "product"
    },
    "origin": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "enum": [
        "MÃ©xico",
        "China",
        "EUA"
      ]
    },
    "images": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "units_sold": {
      "type": "integer",
      "private": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": 0
    },
    "sku": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false
    },
    "seo_descripcion": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "gender": {
      "type": "enumeration",
      "required": true,
      "default": "Hombre",
      "enum": [
        "Hombre",
        "Mujer",
        "Mixto"
      ]
    },
    "description": {
      "type": "richtext"
    },
    "model": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "color": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "average_rating": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": 0,
      "min": 0,
      "max": 5
    },
    "total_reviews": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": 0
    },
    "sustainability_score": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": 5,
      "min": 1,
      "max": 10
    },
    "eco_friendly": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": false
    },
    "estimated_delivery_days": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": 7
    },
    "warranty_months": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": 12
    },
    "video_url": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "recommended_products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "frequently_bought_together": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "view_count": {
      "type": "integer",
      "private": true,
      "default": 0
    },
    "condition": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": "Nuevo",
      "enum": [
        "Nuevo",
        "Como nuevo",
        "Usado - Excelente",
        "Usado - Bueno",
        "Reacondicionado"
      ]
    },
    "size_guide": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "allowedTypes": [
        "images"
      ]
    },
    "profit_margin": {
      "type": "decimal",
      "private": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "product"
    },
    "short_description": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
